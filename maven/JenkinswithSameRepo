pipeline{
    agent any
    
    tools {
  maven 'maven3'
}

    stages{
      
        
        stage("Maven version Stage"){
        steps{
        sh 'mvn -version'
        }
        }
        stage("maven clean stage"){
        steps{
          sh 'mvn -f maven/pom.xml clean package'
        }
    }
}
}
